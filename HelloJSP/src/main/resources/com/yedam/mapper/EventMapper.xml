<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.mapper.EventMapper">

	<select id = "eventList" parameterType="int" resultType="com.yedam.vo.EventVO">
 		select *
 		from tbl_event
 	</select> 	
 	
 	<insert id = "insertEvent" parameterType="com.yedam.vo.EventVO">
 		<selectKey keyProperty="eventNo" resultType ="int" order = "BEFORE">
 			select event_seq.nextval from dual
 		</selectKey>
 		
 		insert into tbl_event(event_no, title, start_date, end_date)
 		values (#{eventNo}, #{eventTitle), #{startDate}, #{endDate})
 	</insert>
 	
</mapper>